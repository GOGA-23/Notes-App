<div class="container-fluid pb-5 mb-5">
  <div class="row">
    <% if(searchResults != '') { %> <% searchResults.forEach(function (note,
    index) { %>

    <a href="/dashboard/item/<%= note._id %>">
      <h4><%= note.title %></h4>
    </a>

    <% }) %> <% } else { %>
    <p class="m-5 p-5 text-center">No items found.</p>
    <% } %>
  </div>
</div>
